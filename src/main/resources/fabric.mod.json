{
  "schemaVersion": 1,
  "id": "psychedelicraft",
  "version": "${version}",
  "name": "Psychedelicraft",
  "description": "Adds drugs into Minecraft. Psychedelicraft specializes on realism and effects. Shaders are being utilized, so keep that in mind.",
  "authors": [
    "Ivorius", "TripleHeadedSheep", "Sollace"
  ],
  "contact": {
    "homepage": "http://www.minecraftforum.net/topic/563257-152",
    "sources": "https://github.com/Sollace/Psychedelicraft",
    "issues": "https://github.com/Sollace/Psychedelicraft/issues"
  },
  "license": "MIT",
  "icon": "logo.png",
  "accessWidener": "psychedelicraft.aw",
  "environment": "*",
  "entrypoints": {
    "main": [
      "ivorius.psychedelicraft.Psychedelicraft"
    ],
    "client": [
      "ivorius.psychedelicraft.client.PsychedelicraftClient"
    ],
    "emi": [
      "ivorius.psychedelicraft.compat.emi.Main"
    ],
    "tla-api": [
      "ivorius.psychedelicraft.compat.tia.Main"
    ],
    "modmenu": [
      "ivorius.psychedelicraft.compat.modmenu.Main"
    ]
  },
  "mixins": [
    "psychedelicraft.mixin.json"
  ],
  "depends": {
    "minecraft": "${minecraftVersion}",
    "fabricloader": "${loaderVersion}",
    "java": ">=17",
    "fabric-api": "${fabricVersion}"
  },
  "suggests": {
    "modmenu": "*"
  },
  "custom": {
    "fabwork": {
      "requiredOn": "*"
    },
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/mt7uwQ4",
        "modmenu.github_releases": "https://github.com/Sollace/Psychedelicraft/releases"
      }
    }
  }
}
